<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ResourceTagDao">

	<insert id="create" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
		insert into ResourceTag values (#{resourceId}, #{tagId}, sysdate)
	</insert>

	<delete id="deleteByResourceId">
		delete from ResourceTag where resourceId = #{resourceId}
	</delete>
	
</mapper>